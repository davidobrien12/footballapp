<p id="notice"><%= notice %></p>
<h2><%= link_to 'Home', teams_path %>
</h2>
<h1>Tickets</h1>
<body style="background-color:#3498DB;">
<table>
  <thead>
    <tr>
      <th>Fixture</th>
      <th>Seat type</th>
      <th>Price</th>
      <th>No of tickets</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.fixture_id %></td>
        <td><%= ticket.seat_type %></td>
        <td><%= ticket.price %></td>
        <td><%= ticket.no_of_tickets %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</body>

<%= link_to 'New Ticket', new_ticket_path %>
